{
  "name": "tanstarter",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cp .env.dev .env && vite dev",
    "build": " cp .env.dev .env && vite build && tsc --noEmit",
    "start": "node .output/server/index.mjs",
    "format": "prettier --write .",
    "db": "drizzle-kit",
    "deps": "pnpm dlx npm-check-updates@latest --interactive --format group",
    "ui": "pnpm dlx shadcn@latest",
    "auth:generate": "pnpm dlx @better-auth/cli@latest generate --y --output ./src/lib/server/schema/auth.schema.ts && prettier --write ./src/lib/server/schema/auth.schema.ts",
    "build:production": " cp .env.production .env && vite build && tsc --noEmit",
    "deploy:cloudflare": "pnpm build:production && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface Env",
    "cf-secret": "wrangler secret"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/postcss": "^4.1.11",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-devtools": "^5.66.0",
    "@tanstack/react-router": "^1.128.3",
    "@tanstack/react-router-devtools": "^1.128.3",
    "@tanstack/react-router-with-query": "^1.128.3",
    "@tanstack/react-start": "^1.128.3",
    "@vitejs/plugin-react": "^4.4.1",
    "better-auth": "^1.2.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.43.1",
    "lucide-react": "^0.507.0",
    "postgres": "^3.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250617.0",
    "@types/node": "^22.15.14",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.31.4",
    "nitro-cloudflare-dev": "^0.2.2",
    "nitropack": "^2.12.3",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rollup": "^4.45.1",
    "tailwindcss": "^4.1.6",
    "tw-animate-css": "^1.2.9",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.20.0"
  }
}
